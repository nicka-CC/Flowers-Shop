model User {
  id             Int     @id @default(autoincrement()) @map("_id")
  permission     Int     @default(0)
  email          String  @unique
  name           String? @default("")
  surname        String? @default("")
  phone          String  @unique
  cart           Cart[]
  history_orders Order[]
  password       String
  code_correctly String
  face           String?
  date_created   String
  date_updated   String
}

model Cart {
  id            Int       @id @default(autoincrement()) @map("_id")
  date          String
  status        String
  date_delivery String
  count_product Int
  product       Product[]
  date_updated  String
  User          User?     @relation(fields: [userId], references: [id])
  userId        Int?
}

model Order {
  id            Int       @id @default(autoincrement()) @map("_id")
  date          String
  status        String
  date_delivery String
  total_service String
  count_product Int
  product       Product[]
  date_created  String
  date_updated  String
  User          User?     @relation(fields: [userId], references: [id])
  userId        Int?
}

model Product {
  id           Int    @id @default(autoincrement()) @map("_id")
  image        String
  name         String
  volume       String
  count        Int
  quantity     Int
  total_count  Int
  date_created String
  date_updated String
  Cart         Cart?  @relation(fields: [cartId], references: [id])
  cartId       Int?
  Order        Order? @relation(fields: [orderId], references: [id])
  orderId      Int?
}

